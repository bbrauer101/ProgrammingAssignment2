complete<- function(directory, id=1:332){
full_list<-list.files(directory,full.names = TRUE)
df<- data.frame(id=numeric(),nobs=numeric())
rows<-data.frame(id=numeric(), nobs=numeric())
for(i in id) {
nobs<-sum(complete.cases(read.csv(full_list[i])))
rows<- c(i,nobs)
df<- rbind(df, rows)
}
}
